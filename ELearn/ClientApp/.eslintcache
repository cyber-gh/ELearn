) => {\n            setLoadingProgress(progress);\n        })\n        let {id} = await addLesson(courseId, rawVideo!.name.split(\".\")[0], location);\n        setLessonId(id);\n        setLocation(location);\n        setSnackbar({message: \"Video uploaded successfully!\", type: \"success\"});\n        setLoading(false);\n        updateLessons();\n    }\n\n    useEffect(() => {\n        uploadVideo();\n    }, [])\n\n    return (\n        <section className = \"video-element\">\n            <div className=\"video\">\n                <p className=\"percentage\">\n                    {loadingProgress.toFixed(1)}%\n                </p>\n                <p className=\"status\">\n                    {loading && \"Uploading ...\"}\n                </p>\n                <LinearProgress variant=\"determinate\" className=\"progress\" value={loadingProgress}/>\n            </div> \n            <div className = \"info\">\n                <p className= \"title\">\n                    {rawVideo.name.split(\".\")[0]}\n                </p>\n                <p className=\"subtitle\">\n                    Uploading: {rawVideo.name}\n                </p>\n            </div>\n        </section>\n    );\n}\n\nexport interface RemoteProps {\n    title: string,\n    videoSrc: string,\n    id: string,\n    courseId: string,\n    updateLessons: () => void,\n}\n\nconst RemoteVideoUploaderElement = ({courseId, updateLessons, title, videoSrc, id}: RemoteProps) => {\n    const {setData: setSnackbar} = useContext(SnackbarContext);\n\n    const handleRemove = async () => {\n        await removeLesson(id);\n        await updateLessons();\n    }\n\n    return (\n        <section className = \"video-element\">\n            <div className=\"preview\">\n                <video src = {videoSrc} />\n            </div>\n            <div className = \"info\">\n                <p className= \"title\">\n                    {title.split(\".\")[0]}\n                </p>\n                <button className=\"remove\" onClick={handleRemove}>Remove Video</button>\n            </div>\n        </section>\n    );\n}\n\nexport {LocalVideoUploaderElement, RemoteVideoUploaderElement}",{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","severity":1,"message":"120","line":2,"column":30,"nodeType":"121","messageId":"122","endLine":2,"endColumn":34},{"ruleId":"123","severity":1,"message":"124","line":10,"column":1,"nodeType":"125","endLine":40,"endColumn":2},{"ruleId":"119","severity":1,"message":"126","line":1,"column":10,"nodeType":"121","messageId":"122","endLine":1,"endColumn":20},{"ruleId":"119","severity":1,"message":"127","line":3,"column":10,"nodeType":"121","messageId":"122","endLine":3,"endColumn":29},{"ruleId":"119","severity":1,"message":"128","line":15,"column":21,"nodeType":"121","messageId":"122","endLine":15,"endColumn":31},{"ruleId":"129","severity":1,"message":"130","line":38,"column":8,"nodeType":"131","endLine":38,"endColumn":10,"suggestions":"132"},{"ruleId":"119","severity":1,"message":"133","line":3,"column":9,"nodeType":"121","messageId":"122","endLine":3,"endColumn":14},{"ruleId":"123","severity":1,"message":"124","line":7,"column":1,"nodeType":"125","endLine":34,"endColumn":2},{"ruleId":"119","severity":1,"message":"134","line":6,"column":10,"nodeType":"121","messageId":"122","endLine":6,"endColumn":18},{"ruleId":"119","severity":1,"message":"135","line":15,"column":11,"nodeType":"121","messageId":"122","endLine":15,"endColumn":16},{"ruleId":"119","severity":1,"message":"136","line":22,"column":45,"nodeType":"121","messageId":"122","endLine":22,"endColumn":56},{"ruleId":"137","severity":1,"message":"138","line":51,"column":21,"nodeType":"139","messageId":"140","endLine":51,"endColumn":23},{"ruleId":"137","severity":1,"message":"138","line":57,"column":14,"nodeType":"139","messageId":"140","endLine":57,"endColumn":16},{"ruleId":"119","severity":1,"message":"141","line":2,"column":9,"nodeType":"121","messageId":"122","endLin